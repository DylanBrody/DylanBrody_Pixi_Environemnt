{
  "name": "pixi",
  "version": "1.0.0",
  "description": "My first PixiJS Project",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/DylanBrody/DylanBrody_Pixi_Environemnt.git"
  },
  "scripts": {
    "start": "vite --open",
    "clean": "rimraf dist/* public/*",
    "prebuild": "run-s clean format:check lint assets types",
    "build": "vite build --base \"./\"",
    "assets": "assetpack",
    "prepreview": "run-s build",
    "preview": "vite preview --open",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "types": "tsc"
  },
  "keywords": [
    "pixi",
    "typescript",
    "live-server"
  ],
  "author": "Dylan Brody",
  "license": "ISC",
  "dependencies": {
    "@assetpack/cli": "^0.8.0",
    "@assetpack/core": "^0.8.0",
    "@assetpack/plugin-compress": "^0.8.0",
    "@assetpack/plugin-ffmpeg": "^0.8.0",
    "@assetpack/plugin-json": "^0.8.0",
    "@assetpack/plugin-manifest": "^0.8.0",
    "@assetpack/plugin-mipmap": "^0.8.0",
    "@assetpack/plugin-texture-packer": "^0.8.0",
    "@assetpack/plugin-webfont": "^0.8.0",
    "@pixi/sound": "^6.0.0",
    "@pixi/ui": "^0.10.5",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "npm-run-all": "^4.1.5",
    "pixi-filters": "^6.0.0",
    "pixi.js": "^8.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typed-signals": "^2.5.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  }
}